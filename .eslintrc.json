{
    "env": {
        //"browser": true,
        //"commonjs": true,
        "node": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "plugins": [
        "prettier"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "prettier/prettier": ["error"],
        "no-plusplus": ["error", { "allowForLoopAfterthoughts": true }],
        "no-unused-vars": ["error", { "argsIgnorePattern": "^_" }]
    }
}
